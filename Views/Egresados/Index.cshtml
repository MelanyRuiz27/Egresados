@model IEnumerable<EgresadosU.Models.Egresados>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Home/Index.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FechaEgreso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TarjetaProfesional)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Programas.Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Personas.Documento)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FechaEgreso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TarjetaProfesional)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Programas.Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Personas.Documento)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
